{
  "name": "json-compact-prettifier",
  "displayName": "JSON Compact Formatter/Prettifier",
  "description": "Prettify JSON Without Wasting Space",
  "version": "1.3.7",
  "publisher": "inadarei",
  "engines": {"vscode": "^1.69.0"},
  "categories": ["Other"],
  "icon": "img/json-icon-logo.png",
  "galleryBanner": {"color": "#EFF1F3", "theme": "light"},
  "repository": {
    "type": "git",
    "url": "https://github.com/inadarei/json-compact-formatter"
  },
  "activationEvents": ["onCommand:json-compact-prettifier.prettyFormatCompact"],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "json-compact-prettifier.prettyFormatCompact",
        "title": "Format: Compact Pretty JSON"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {"@types/node": "^12.12.0", "@types/vscode": "^1.32.0"},
  "dependencies": {
    "json-stringify-pretty-compact": "3.0.0",
    "jsonlint-mod": "^1.7.6"
  }
}
